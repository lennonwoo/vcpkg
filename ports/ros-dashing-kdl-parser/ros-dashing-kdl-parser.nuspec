<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-kdl-parser</id>
    <version>2.2.0</version>
    <title>ros-dashing-kdl-parser</title>
    <authors>Chris Lalancette</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The Kinematics and Dynamics Library (KDL) defines a tree structure to represent the kinematic and dynamic parameters of a robot mechanism.
 kdl_parser provides tools to construct a KDL tree from an XML robot representation in URDF.</description>
    <dependencies>
		<dependency id="ros-dashing-orocos-kdl" version="1.3.0"/>
		<dependency id="ros-dashing-tinyxml-vendor"/>
		<dependency id="ros-dashing-urdf"/>
		<dependency id="ros-dashing-urdfdom-headers"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
