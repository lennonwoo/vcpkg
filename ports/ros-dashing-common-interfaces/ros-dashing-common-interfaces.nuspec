<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-common-interfaces</id>
    <version>0.7.0</version>
    <title>ros-dashing-common-interfaces</title>
    <authors>Scott K Logan</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>common_interfaces contains messages and services that are widely used by other ROS packages.</description>
    <dependencies>
		<dependency id="ros-dashing-actionlib-msgs"/>
		<dependency id="ros-dashing-builtin-interfaces"/>
		<dependency id="ros-dashing-diagnostic-msgs"/>
		<dependency id="ros-dashing-geometry-msgs"/>
		<dependency id="ros-dashing-nav-msgs"/>
		<dependency id="ros-dashing-sensor-msgs"/>
		<dependency id="ros-dashing-shape-msgs"/>
		<dependency id="ros-dashing-std-msgs"/>
		<dependency id="ros-dashing-std-srvs"/>
		<dependency id="ros-dashing-stereo-msgs"/>
		<dependency id="ros-dashing-trajectory-msgs"/>
		<dependency id="ros-dashing-visualization-msgs"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
