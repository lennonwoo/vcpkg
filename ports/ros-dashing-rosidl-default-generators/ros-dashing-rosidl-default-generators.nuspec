<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rosidl-default-generators</id>
    <version>0.7.0</version>
    <title>ros-dashing-rosidl-default-generators</title>
    <authors>Dirk Thomas</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A configuration package defining the default ROS interface generators.</description>
    <dependencies>
		<dependency id="ros-dashing-ament-cmake-core"/>
		<dependency id="ros-dashing-rosidl-cmake"/>
		<dependency id="ros-dashing-rosidl-generator-c"/>
		<dependency id="ros-dashing-rosidl-generator-cpp"/>
		<dependency id="ros-dashing-rosidl-generator-py"/>
		<dependency id="ros-dashing-rosidl-typesupport-c"/>
		<dependency id="ros-dashing-rosidl-typesupport-cpp"/>
		<dependency id="ros-dashing-rosidl-typesupport-introspection-c"/>
		<dependency id="ros-dashing-rosidl-typesupport-introspection-cpp"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
