<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-ament-lint-common</id>
    <version>0.7.3</version>
    <title>ros-dashing-ament-lint-common</title>
    <authors>Dirk Thomas</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The list of commonly used linters in the ament buildsytem in CMake.</description>
    <dependencies>
		<dependency id="ros-dashing-ament-cmake-copyright"/>
		<dependency id="ros-dashing-ament-cmake-core"/>
		<dependency id="ros-dashing-ament-cmake-cppcheck"/>
		<dependency id="ros-dashing-ament-cmake-cpplint"/>
		<dependency id="ros-dashing-ament-cmake-flake8"/>
		<dependency id="ros-dashing-ament-cmake-lint-cmake"/>
		<dependency id="ros-dashing-ament-cmake-pep257"/>
		<dependency id="ros-dashing-ament-cmake-uncrustify"/>
		<dependency id="ros-dashing-ament-cmake-xmllint"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
