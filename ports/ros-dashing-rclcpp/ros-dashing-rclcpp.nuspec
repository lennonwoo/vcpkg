<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rclcpp</id>
    <version>0.7.6</version>
    <title>ros-dashing-rclcpp</title>
    <authors>Dirk Thomas</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The ROS client library in C++.</description>
    <dependencies>
		<dependency id="ros-dashing-ament-cmake"/>
		<dependency id="ros-dashing-builtin-interfaces"/>
		<dependency id="ros-dashing-rcl"/>
		<dependency id="ros-dashing-rcl-interfaces"/>
		<dependency id="ros-dashing-rcl-yaml-param-parser"/>
		<dependency id="ros-dashing-rmw"/>
		<dependency id="ros-dashing-rmw-implementation"/>
		<dependency id="ros-dashing-rosgraph-msgs"/>
		<dependency id="ros-dashing-rosidl-generator-cpp"/>
		<dependency id="ros-dashing-rosidl-typesupport-c"/>
		<dependency id="ros-dashing-rosidl-typesupport-cpp"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
