<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-ros2launch</id>
    <version>0.8.4</version>
    <title>ros-dashing-ros2launch</title>
    <authors>William Woodall</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The launch command for ROS 2 command line tools.</description>
    <dependencies>
		<dependency id="ros-dashing-ament-index-python"/>
		<dependency id="ros-dashing-launch"/>
		<dependency id="ros-dashing-launch-ros"/>
		<dependency id="ros-dashing-ros2cli"/>
		<dependency id="ros-dashing-ros2pkg"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
