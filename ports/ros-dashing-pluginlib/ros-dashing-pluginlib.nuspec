<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-pluginlib</id>
    <version>2.3.1</version>
    <title>ros-dashing-pluginlib</title>
    <authors>Steven! Ragnar√∂k</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The pluginlib package provides tools for writing and dynamically loading plugins using the ROS build infrastructure.
 To work, these tools require plugin providers to register their plugins in the package.xml of their package.</description>
    <dependencies>
		<dependency id="ros-dashing-ament-index-cpp"/>
		<dependency id="ros-dashing-class-loader"/>
		<dependency id="ros-dashing-rcutils"/>
		<dependency id="ros-dashing-tinyxml2-vendor"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
