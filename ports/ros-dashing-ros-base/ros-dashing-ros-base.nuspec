<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-ros-base</id>
    <version>0.7.2</version>
    <title>ros-dashing-ros-base</title>
    <authors>Steven! Ragnar√∂k</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A package which extends 'ros_core' and includes other basic functionalities like tf2 and urdf.</description>
    <dependencies>
		<dependency id="ros-dashing-kdl-parser"/>
		<dependency id="ros-dashing-robot-state-publisher"/>
		<dependency id="ros-dashing-ros-core"/>
		<dependency id="ros-dashing-tf2"/>
		<dependency id="ros-dashing-tf2-eigen"/>
		<dependency id="ros-dashing-tf2-geometry-msgs"/>
		<dependency id="ros-dashing-tf2-kdl"/>
		<dependency id="ros-dashing-tf2-ros"/>
		<dependency id="ros-dashing-urdf"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
