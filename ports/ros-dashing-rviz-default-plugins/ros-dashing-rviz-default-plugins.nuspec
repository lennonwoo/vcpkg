<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rviz-default-plugins</id>
    <version>6.1.2</version>
    <title>ros-dashing-rviz-default-plugins</title>
    <authors>William Woodall</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Several default plugins for rviz to cover the basic functionality.</description>
<dependencies>
      <dependency id="ros-dashing-geometry-msgs"/>
      <dependency id="ros-dashing-laser-geometry"/>
      <dependency id="ros-dashing-map-msgs"/>
      <dependency id="ros-dashing-nav-msgs"/>
      <dependency id="ros-dashing-pluginlib"/>
      <dependency id="ros-dashing-rclcpp"/>
      <dependency id="ros-dashing-resource-retriever"/>
      <dependency id="ros-dashing-rviz-common"/>
      <dependency id="ros-dashing-rviz-rendering"/>
      <dependency id="ros-dashing-tinyxml-vendor"/>
      <dependency id="ros-dashing-urdf"/>
      <dependency id="ros-dashing-visualization-msgs"/>
      <dependency id="ros-dashing-ros-workspace"/>
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>