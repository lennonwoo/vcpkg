<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-qt-gui-core</id>
    <version>1.0.6</version>
    <title>ros-dashing-qt-gui-core</title>
    <authors>Dirk Thomas</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Integration of the ROS package system and ROS-specific plugins for a Qt-based GUI.</description>
    <dependencies>
		<dependency id="ros-dashing-qt-dotgraph" version="0.3.0"/>
		<dependency id="ros-dashing-qt-gui" version="0.3.0"/>
		<dependency id="ros-dashing-qt-gui-app" version="0.3.0"/>
		<dependency id="ros-dashing-qt-gui-cpp" version="0.3.0"/>
		<dependency id="ros-dashing-qt-gui-py-common" version="0.3.0"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
