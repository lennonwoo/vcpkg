<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-uncrustify-vendor</id>
    <version>1.2.0</version>
    <title>ros-dashing-uncrustify-vendor</title>
    <authors>Steven! Ragnar√∂k</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Wrapper around uncrustify, providing nothing but a dependency on uncrustify, on some systems.
 On others, it provides an ExternalProject build of uncrustify.</description>
<dependencies>
      <dependency id="ros-dashing-ros-workspace"/>
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>