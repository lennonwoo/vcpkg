<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-demo-nodes-cpp</id>
    <version>0.7.8</version>
    <title>ros-dashing-demo-nodes-cpp</title>
    <authors>William Woodall</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>C++ nodes which were previously in the ros2/examples repository but are now just used for demo purposes.</description>
    <dependencies>
		<dependency id="ros-dashing-example-interfaces"/>
		<dependency id="ros-dashing-launch-ros"/>
		<dependency id="ros-dashing-rclcpp"/>
		<dependency id="ros-dashing-rcutils"/>
		<dependency id="ros-dashing-rmw"/>
		<dependency id="ros-dashing-std-msgs"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
