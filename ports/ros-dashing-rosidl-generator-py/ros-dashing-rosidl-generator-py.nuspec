<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rosidl-generator-py</id>
    <version>0.7.7</version>
    <title>ros-dashing-rosidl-generator-py</title>
    <authors>Scott K Logan</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Generate the ROS interfaces in Python.</description>
    <dependencies>
		<dependency id="ros-dashing-ament-cmake"/>
		<dependency id="ros-dashing-ament-index-python"/>
		<dependency id="ros-dashing-python-cmake-module"/>
		<dependency id="ros-dashing-rmw"/>
		<dependency id="ros-dashing-rmw-implementation"/>
		<dependency id="ros-dashing-rmw-implementation-cmake"/>
		<dependency id="ros-dashing-rosidl-cmake"/>
		<dependency id="ros-dashing-rosidl-generator-c"/>
		<dependency id="ros-dashing-rosidl-parser"/>
		<dependency id="ros-dashing-rosidl-typesupport-c"/>
		<dependency id="ros-dashing-rosidl-typesupport-interface"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
