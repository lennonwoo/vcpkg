<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-demo-nodes-cpp-native</id>
    <version>0.7.8</version>
    <title>ros-dashing-demo-nodes-cpp-native</title>
    <authors>Dirk Thomas</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>C++ nodes which access the native handles of the rmw implemenation.</description>
    <dependencies>
		<dependency id="ros-dashing-rclcpp"/>
		<dependency id="ros-dashing-rmw-fastrtps-cpp"/>
		<dependency id="ros-dashing-std-msgs"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
