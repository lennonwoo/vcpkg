<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rviz-rendering-tests</id>
    <version>6.1.2</version>
    <title>ros-dashing-rviz-rendering-tests</title>
    <authors>William Woodall</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Example plugin for RViz - documents and tests RViz plugin development</description>
    <dependencies>
		<dependency id="ros-dashing-resource-retriever"/>
		<dependency id="ros-dashing-rviz-rendering"/>
		<dependency id="ros-dashing-ros-workspace"/>

