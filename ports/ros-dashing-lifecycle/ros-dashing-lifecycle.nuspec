<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-lifecycle</id>
    <version>0.7.8</version>
    <title>ros-dashing-lifecycle</title>
    <authors>Karsten Knese</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Package containing demos for lifecycle implementation</description>
    <dependencies>
		<dependency id="ros-dashing-lifecycle-msgs"/>
		<dependency id="ros-dashing-rclcpp-lifecycle"/>
		<dependency id="ros-dashing-ros2run"/>
		<dependency id="ros-dashing-std-msgs"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
