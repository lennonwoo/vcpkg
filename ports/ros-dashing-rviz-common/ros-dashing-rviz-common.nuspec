<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rviz-common</id>
    <version>6.1.2</version>
    <title>ros-dashing-rviz-common</title>
    <authors>William Woodall</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Common rviz API, used by rviz plugins and applications.</description>
<dependencies>
      <dependency id="ros-dashing-geometry-msgs"/>
      <dependency id="ros-dashing-pluginlib"/>
      <dependency id="ros-dashing-rclcpp"/>
      <dependency id="ros-dashing-resource-retriever"/>
      <dependency id="ros-dashing-rviz-assimp-vendor"/>
      <dependency id="ros-dashing-rviz-ogre-vendor"/>
      <dependency id="ros-dashing-rviz-rendering"/>
      <dependency id="ros-dashing-sensor-msgs"/>
      <dependency id="ros-dashing-std-msgs"/>
      <dependency id="ros-dashing-tf2"/>
      <dependency id="ros-dashing-tf2-geometry-msgs"/>
      <dependency id="ros-dashing-tf2-ros"/>
      <dependency id="ros-dashing-tinyxml-vendor"/>
      <dependency id="ros-dashing-urdf"/>
      <dependency id="ros-dashing-yaml-cpp-vendor"/>
      <dependency id="ros-dashing-ros-workspace"/>
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>