<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-ament-cmake</id>
    <version>0.7.3</version>
    <title>ros-dashing-ament-cmake</title>
    <authors>Dirk Thomas</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The entry point package for the ament buildsystem in CMake.</description>
    <dependencies>
		<dependency id="ros-dashing-ament-cmake-core"/>
		<dependency id="ros-dashing-ament-cmake-export-definitions"/>
		<dependency id="ros-dashing-ament-cmake-export-dependencies"/>
		<dependency id="ros-dashing-ament-cmake-export-include-directories"/>
		<dependency id="ros-dashing-ament-cmake-export-interfaces"/>
		<dependency id="ros-dashing-ament-cmake-export-libraries"/>
		<dependency id="ros-dashing-ament-cmake-export-link-flags"/>
		<dependency id="ros-dashing-ament-cmake-libraries"/>
		<dependency id="ros-dashing-ament-cmake-python"/>
		<dependency id="ros-dashing-ament-cmake-target-dependencies"/>
		<dependency id="ros-dashing-ament-cmake-test"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
