<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-pendulum-control</id>
    <version>0.7.8</version>
    <title>ros-dashing-pendulum-control</title>
    <authors>Michael Carroll</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Demonstrates ROS 2's realtime capabilities with a simulated inverted pendulum.</description>
<dependencies>
      <dependency id="ros-dashing-pendulum-msgs"/>
      <dependency id="ros-dashing-rclcpp"/>
      <dependency id="ros-dashing-rttest"/>
      <dependency id="ros-dashing-tlsf-cpp"/>
      <dependency id="ros-dashing-ros-workspace"/>
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>