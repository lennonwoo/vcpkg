<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-launch-testing-ros</id>
    <version>0.8.4</version>
    <title>ros-dashing-launch-testing-ros</title>
    <authors>Pete Baughman</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A package providing utilities for writing ROS2 enabled launch tests.</description>
    <dependencies>
		<dependency id="ros-dashing-launch-ros"/>
		<dependency id="ros-dashing-launch-testing"/>
		<dependency id="ros-dashing-rclpy"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
