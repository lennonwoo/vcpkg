<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-robot-state-publisher</id>
    <version>2.2.3</version>
    <title>ros-dashing-robot-state-publisher</title>
    <authors>Karsten Knese</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ROS2 version of the robot_state_publisher package</description>
    <dependencies>
		<dependency id="ros-dashing-geometry-msgs"/>
		<dependency id="ros-dashing-kdl-parser"/>
		<dependency id="ros-dashing-orocos-kdl"/>
		<dependency id="ros-dashing-rclcpp"/>
		<dependency id="ros-dashing-sensor-msgs"/>
		<dependency id="ros-dashing-tf2-ros"/>
		<dependency id="ros-dashing-urdf"/>
		<dependency id="ros-dashing-urdfdom-headers"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
